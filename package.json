{
  "name": "3rd-byob",
  "version": "1.0.0",
  "main": "gulpfile.js",
  "type": "module",
  "scripts": {
    "build": "npm run clean && npm run copy && npm run webpack",
    "copy": "cp index.html dist/ && cp -R assets/ dist/assets/",
    "babel": "babel --presets es2015 ./assets/js/main.js -o ./dist/assets/js/main.js",
    "clean": "rm -rf dist && mkdir dist",
    "webpack": "webpack --config webpack.config.js",
    "start": "http-server -p 9002"
  },
  "author": "ZURB <foundation@zurb.com>",
  "license": "MIT",
  "dependencies": {
    "@sitespirit/vtb-transformer": "^1.0.0",
    "vtb-messenger": "0.0.1",
    "cross-env": "^7.0.2",
    "ejs": "^3.1.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "http-server": "^0.12.3",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zurb/foundation-sites-template.git"
  },
  "private": true
}
